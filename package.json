  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/react-hooks": "^5.1.3",
    "bootstrap": "^4.6.0",
    "canvas": "^2.8.0",
    "chart.js": "^2.9.4",
    "cookie_js": "^1.4.2",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^4.1.1",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.6.4",
    "react-cookie-consent": "^4.1.0",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-i18next": "^11.12.0",
    "react-router-dom": "^5.3.0",
    "react-select": "^4.3.1",
    "react-social-login": "3.4.10",
    "react-social-login-buttons": "3.1.0",
    "styled-components": "^5.3.1",
    "use-persisted-reducer": "^2.1.0",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "target-lint": "./scripts/target-lint",
    "lint": "eslint \"./src/**/*.+(js|jsx)\" --max-warnings=0",
    "clean": "eslint --fix \"./src/**/*.+(js|jsx)\"; prettier --write \"./src/**/*.+(js|jsx|json|yml|yaml|css|scss|md|vue)\"",
    "coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --coverageDirectory=coverage --watchAll=false"
  },
  "jest": {
    "resetMocks": false,
    "coverageReporters": [
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "src/serviceWorker.js",
      "src/service-worker.js",
      "src/setupTests.js",
      "src/configuration/*",
      "src/test.fixtures/*",
      "src/providers/api/test.fixtures/*",
      ".hook.mock.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-unused-imports": "^1.1.5",
    "firebase": "^8.10.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-styled-components": "^7.0.5",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.4.1",
    "react-scripts": "^4.0.3"
  }
}
